package org.shoper.commons.exception;

/**
 * Created by ShawnShoper on 16/8/3.
 */
public class ShoperException extends Exception{
    private static final long serialVersionUID = -5826285069099640009L;
    public ShoperException(String message)
    {
        super(message);
    }
    public ShoperException()
    {
        super();
    }
    public ShoperException(Exception e)
    {
        addSuppressed(e);
    }
}
