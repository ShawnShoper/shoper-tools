package org.shoper.commons;

/**
 *
 */
public class ImageInfo {
	private int width;
	private int heigt;

	public int getHeigt () {
		return heigt;
	}

	public void setHeigt (int heigt) {
		this.heigt = heigt;
	}

	public ImageInfo (int width, int heigt) {
		this.width = width;
		this.heigt = heigt;
	}

	public int getWidth () {
		return width;
	}

	public void setWidth (int width) {
		this.width = width;
	}
}